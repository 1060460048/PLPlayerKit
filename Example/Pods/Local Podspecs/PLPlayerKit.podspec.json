{
  "name": "PLPlayerKit",
  "version": "2.3.0",
  "summary": "Pili iOS video player SDK, RTMP, HLS video streaming supported.",
  "homepage": "https://github.com/pili-engineering/PLPlayerKit",
  "license": "Apache License, Version 2.0",
  "authors": {
    "0dayZh": "0day.zh@gmail.com"
  },
  "source": {
    "git": "https://github.com/pili-engineering/PLPlayerKit.git",
    "tag": "v2.3.0"
  },
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "public_header_files": "Pod/Library/include/PLPlayerKit/*.h",
  "source_files": "Pod/Library/include/**/*.[h|m]",
  "dependencies": {
    "pili-librtmp": [
      "~> 1.0.5"
    ],
    "HappyDNS": [
      "~> 0.3.10"
    ]
  },
  "frameworks": [
    "UIKit",
    "Foundation",
    "CoreGraphics",
    "MediaPlayer",
    "CoreAudio",
    "AudioToolbox",
    "Accelerate",
    "QuartzCore",
    "OpenGLES",
    "AVFoundation"
  ],
  "libraries": [
    "c++",
    "z",
    "bz2",
    "iconv"
  ],
  "default_subspecs": "precompiled",
  "subspecs": [
    {
      "name": "precompiled",
      "preserve_paths": [
        "Pod/Library/include/PLPlayerKit/*.[h|m]",
        "Pod/Library/lib/*.a"
      ],
      "vendored_libraries": "Pod/Library/lib/*.a",
      "xcconfig": {
        "HEADER_SEARCH_PATHS": "${PODS_ROOT}/pili-ffmpeg/pili-ffmpeg/include"
      },
      "dependencies": {
        "pili-ffmpeg": [
          "~> 3.1.0"
        ]
      }
    },
    {
      "name": "noffmpeg",
      "public_header_files": "Pod/Library/include/PLPlayerKit/*.h",
      "source_files": "Pod/Library/include/**/*.[h|m]",
      "preserve_paths": [
        "Pod/Library/include/PLPlayerKit/*.[h|m]",
        "Pod/Library/lib/*.a"
      ],
      "vendored_libraries": "Pod/Library/lib/*.a",
      "xcconfig": {
        "HEADER_SEARCH_PATHS": "${PODS_ROOT}/../ffmpeg/include",
        "LIBRARY_SEARCH_PATHS": "${PODS_ROOT}/../ffmpeg/lib"
      }
    }
  ]
}
